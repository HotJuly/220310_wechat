/* pages/index/index.wxss */
/* 如果存在导航栏,导航栏不属于当前页面 */
page{
  height:100%;
}
.indexContainer{
  height:100%;
  display:flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.indexContainer .avatarUrl{
  /* 在iphone6下,页面宽度是375px,小程序中750rpx等于屏幕的宽度
    所以1px = 2rpx
  */
  width:400rpx;
  height:400rpx;
  border-radius: 50%;
}